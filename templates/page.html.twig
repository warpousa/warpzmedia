{# page.html.twig for warpedZ #}
{# Page top (toolbar, messages, etc.) is rendered in html.html.twig as {{ page_top }}. #}
<header role="banner">
  <div class="site-branding">
    {% if logo %}
      <a href="{{ path('<front>') }}" rel="home" class="site-logo">
        <img src="{{ logo }}" alt="{{ site_name|default('Home') }}">
      </a>
    {% endif %}
    {% if site_name %}
      <a href="{{ path('<front>') }}" rel="home" class="site-name">
        {{ site_name }}
      </a>
    {% endif %}
  </div>
  {% if page.header %}
    <div class="region-header">
      {{ page.header }}
    </div>
  {% endif %}
  {% if page.primary_menu %}
    <nav role="navigation" aria-label="Main navigation">
      {{ page.primary_menu }}
    </nav>
  {% endif %}
</header>
<main role="main">
  <a id="main-content" tabindex="-1"></a>
  {# Flavor toggles live near the top of main content. #}
  {% include '@warpedZ/partials/flavor-toggles.html.twig' %}
  {{ page.content }}
</main>
{% if page.footer %}
  <footer role="contentinfo">
    {{ page.footer }}
  </footer>
{% endif %}